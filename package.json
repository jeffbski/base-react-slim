{
  "name": "PROJECT_NAME",
  "description": "",
  "version": "0.1.0",
  "author": "Jeff Barczewski <info@codewinds.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/jeffbski/PROJECT_NAME.git"
  },
  "bugs": {
    "url": "http://github.com/jeffbski/PROJECT_NAME/issues"
  },
  "license": "MIT",
  "browser": {
    "main": "src/browser.jsx"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "scripts": {
    "create-dirs": "mkdirp dist",
    "build": "run-p -c build:*",
    "build:css": "lessc style/site.less | postcss -c postcss.json | ntee dist/site.css | cleancss | ntee dist/site.min.css | ngzip > dist/site.min.css.gz",
    "build:js": "cross-env BABEL_ENV=commonjs webpack  && uglifyjs dist/main.js -c warnings=false -m | ntee dist/main.min.js | ngzip > dist/main.min.js.gz",
    "lint": "eslint --ext .js --ext .jsx src",
    "prebuild": "rimraf dist/* && run-s create-dirs",
    "postbuild": "echo \"Finished\"",
    "prepublish": "run-s prod-build",
    "prewatch": "run-s create-dirs",
    "prewatch-js": "rimraf dist/main.min.js dist/main.min.js.gz",
    "prod-build": "npm run build --production",
    "start": "run-s watch",
    "watch": "run-p -c watch:*",
    "watch:browser": "wait-on dist/main.js && browser-sync start --config bs-config.js",
    "watch:css": "run-s build:css && chokidar \"style/**/*\" -i \"*.flycheck.*\" -c \"run-s build:css\"",
    "watch:js": "cross-env BABEL_ENV=commonjs webpack --watch"
  },
  "engines": {
    "node": ">=4.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.10.1",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-for-of": "^6.8.0",
    "babel-plugin-transform-es2015-function-name": "^6.9.0",
    "babel-plugin-transform-es2015-literals": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-es2015-object-super": "^6.8.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.8.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.8.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.11.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.11.6",
    "browser-sync": "^2.14.0",
    "chokidar-cli": "^1.1.0",
    "clean-css": "^3.4.19",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-react": "^6.1.2",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "mkdirp": "^0.5.1",
    "ngzip": "^1.0.0",
    "npm-run-all": "^3.0.0",
    "ntee": "^1.1.5",
    "postcss-cli": "^2.5.2",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.7.3",
    "wait-on": "^1.5.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "core-js": "^2.4.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "keywords": []
}
